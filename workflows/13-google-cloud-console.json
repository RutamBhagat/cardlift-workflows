{
    "category": "productivity",
    "enabled": false,
    "historyDetectionUrl": "",
    "hostname": "console.cloud.google.com",
    "id": "",
    "name": "Google Cloud Console",
    "runInFocusMode": false,
    "steps": [
        {
            "executionType": "separate",
            "help": {
                "message": "We're having trouble finding the billing page.",
                "title": "Trouble Finding Billing Page"
            },
            "info": {
                "message": "We are filling your billing details",
                "title": "Filling Billing Details"
            },
            "isRequired": true,
            "operation": {
                "ignoreFailure": true,
                "type": "navigate",
                "url": "https://console.cloud.google.com/welcome"
            }
        },
        {
            "conditional": [
                {
                    "urlSelectors": [
                        "https://www.figma.com/login"
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "message": "Please sign in to your Google Cloud Console account.",
                "successConditional": [
                    {
                        "urlSelectors": [
                            "https://console.cloud.google.com/welcome"
                        ]
                    }
                ],
                "type": "await_user_help"
            }
        },
        {
            "executionType": "linked",
            "operation": {
                "ignoreFailure": true,
                "type": "navigate",
                "url": "https://console.cloud.google.com/welcome"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "div.page-title:contains('Billing')"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "events": [
                    "click"
                ],
                "selector": "div.page-title:contains('Billing')",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "a[aria-label*='Payment method']"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "events": [
                    "click"
                ],
                "selector": "a[aria-label*='Payment method']",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "a:contains('Add a primary payment method')"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "events": [
                    "click"
                ],
                "selector": "a:contains('Add a primary payment method')",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "div.b3-additional-instrument-option-label:contains('Add credit or debit card')"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "events": [
                    "click"
                ],
                "selector": "div.b3-additional-instrument-option-label:contains('Add credit or debit card')",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "span[class=\"b3-additional-instrument-option-label\"][data-was-visible=\"true\"]:contains(\"Add credit or debit card\")"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "cardProperty": "cardNumber",
                "events": [
                    "fillInputData",
                    "input"
                ],
                "selector": "span[class=\"b3-additional-instrument-option-label\"][data-was-visible=\"true\"]:contains(\"Add credit or debit card\")",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "watchedElementSelectors": [
                        {
                            "sizzleSelector": "div:textEquals(\"Successfully updated your payment method.\")"
                        }
                    ]
                }
            ],
            "executionType": "separate"
        }
    ],
    "testing": true,
    "type": "BUSINESS"
}