{
    "category": "productivity",
    "enabled": false,
    "historyDetectionUrl": "",
    "hostname": "doordash.com",
    "id": "",
    "name": "Doordash",
    "runInFocusMode": false,
    "steps": [
        {
            "executionType": "separate",
            "help": {
                "message": "We're having trouble finding the billing page.",
                "title": "Trouble Finding Billing Page"
            },
            "info": {
                "message": "We are filling your billing details",
                "title": "Filling Billing Details"
            },
            "isRequired": true,
            "operation": {
                "ignoreFailure": true,
                "type": "navigate",
                "url": "https://www.doordash.com/consumer/payment"
            }
        },
        {
            "conditional": [
                {
                    "urlSelectors": [
                        "https://identity.doordash.com/auth"
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "message": "Please sign in to your Doordash account.",
                "successConditional": [
                    {
                        "urlSelectors": [
                            "https://www.doordash.com/consumer/payment"
                        ]
                    }
                ],
                "type": "await_user_help"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "[data-anchor-id=\"PaymentMethodAdd\"]"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "events": [
                    "click"
                ],
                "selector": "[data-anchor-id=\"PaymentMethodAdd\"]",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "iframe[title='Secure card number input frame']"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "type": "autofill_card_data"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "iframe[title='Secure card security code input frame']"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "cardProperty": "cardCvc",
                "events": [
                    "fillInputData",
                    "input"
                ],
                "selector": "input[name='vgs-collect-cvv-field']",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "iframe[title='Secure card expiration date input frame']"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "cardProperty": "cardExpirationDate",
                "events": [
                    "fillInputData",
                    "input"
                ],
                "selector": "input[placeholder='MM / YY']",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "iframe[title='Secure text input frame']"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "cardProperty": "zip",
                "events": [
                    "fillInputData",
                    "input"
                ],
                "selector": "input[placeholder='Zip Code']",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "button[data-testid='AddCardButton']"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "events": [
                    "click"
                ],
                "selector": "button[data-testid='AddCardButton']",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "div:contains(\"successfully added\")"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "isRequired": true
        },
        {
            "conditional": [
                {
                    "watchedElementSelectors": [
                        {
                            "sizzleSelector": ""
                        }
                    ]
                }
            ],
            "executionType": "separate"
        }
    ],
    "testing": true,
    "type": "BUSINESS"
}