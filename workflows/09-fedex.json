{
    "category": "productivity",
    "enabled": false,
    "historyDetectionUrl": "",
    "hostname": "fedex.com",
    "id": "",
    "name": "Fedex",
    "runInFocusMode": false,
    "steps": [
        {
            "executionType": "separate",
            "help": {
                "message": "We're having trouble finding the billing page.",
                "title": "Trouble Finding Billing Page"
            },
            "info": {
                "message": "We are filling your billing details",
                "title": "Filling Billing Details"
            },
            "isRequired": true,
            "operation": {
                "ignoreFailure": true,
                "type": "navigate",
                "url": "https://www.fedex.com/online/billing/cbs/summary"
            }
        },
        {
            "conditional": [
                {
                    "urlSelectors": [
                        "https://www.fedex.com/secure-login/"
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "message": "Please sign in to your Fedex account.",
                "successConditional": [
                    {
                        "urlSelectors": [
                            "https://www.fedex.com/online/billing/cbs/summary"
                        ]
                    }
                ],
                "type": "await_user_help"
            }
        },
        {
            "executionType": "linked",
            "operation": {
                "ignoreFailure": true,
                "type": "navigate",
                "url": "https://www.fedex.com/online/billing/cbs/summary"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "a.fdx-c-button.fdx-c-button--text:contains('Cancel')"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "events": [
                    "click"
                ],
                "selector": "a.fdx-c-button.fdx-c-button--text:contains('Cancel')",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "span.fdx-c-icon-bar__list__item__button__text:textEquals('Manage Payments')"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "events": [
                    "click"
                ],
                "selector": "span.fdx-c-icon-bar__list__item__button__text:textEquals('Manage Payments')",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "button.fdx-c-icon-bar__list-sub__item__button:textEquals(' Payment Profiles ')"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "events": [
                    "click"
                ],
                "selector": "button.fdx-c-icon-bar__list-sub__item__button:textEquals(' Payment Profiles ')",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "button#edit_profile_button span:textEquals('edit')"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "events": [
                    "click"
                ],
                "selector": "button#edit_profile_button span:textEquals('edit')",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "watchedElementSelectors": [
                        {
                            "sizzleSelector": ""
                        }
                    ]
                }
            ],
            "executionType": "separate"
        }
    ],
    "testing": true,
    "type": "BUSINESS"
}