{
    "category": "productivity",
    "enabled": false,
    "historyDetectionUrl": "",
    "hostname": "console.cloud.google.com",
    "id": "",
    "name": "Google Cloud Console",
    "runInFocusMode": false,
    "steps": [
        {
            "executionType": "separate",
            "help": {
                "message": "We're having trouble finding the billing page.",
                "title": "Trouble Finding Billing Page"
            },
            "info": {
                "message": "We are filling your billing details",
                "title": "Filling Billing Details"
            },
            "isRequired": true,
            "operation": {
                "ignoreFailure": true,
                "type": "navigate",
                "url": "https://console.cloud.google.com/welcome"
            }
        },
        {
            "conditional": [
                {
                    "urlSelectors": [
                        "https://accounts.google.com/"
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "message": "Please sign in to your Google Cloud Console account.",
                "successConditional": [
                    {
                        "urlSelectors": [
                            "https://console.cloud.google.com/welcome"
                        ]
                    }
                ],
                "type": "await_user_help"
            }
        },
        {
            "executionType": "separate",
            "operation": {
                "ignoreFailure": true,
                "type": "navigate",
                "url": "https://console.cloud.google.com/welcome"
            }
        },
        {
            "executionType": "linked",
            "operation": {
                "ignoreFailure": true,
                "type": "navigate",
                "url": "https://console.cloud.google.com/welcome"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "div.page-title:contains('Billing')"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "events": [
                    "click"
                ],
                "selector": "div.page-title:contains('Billing')",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "a[aria-label*='Payment method']"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "events": [
                    "click"
                ],
                "selector": "a[aria-label*='Payment method']",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "div.b3-payment-methods-instrument-details-section-ranked:has(span.b3id-form-title-content:textEquals('Primary')) a.b3id-widget-link:has(span:textEquals('Remove'))"
                        }
                    ]
                }
            ],
            "executionType": "separate", 
            "operation": {
                "events": [
                    "click"
                ],
                "selector": "div.b3-payment-methods-instrument-details-section-ranked:has(span.b3id-form-title-content:textEquals('Primary')) a.b3id-widget-link:has(span:textEquals('Remove'))",
                "type": "fire_events"  
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "a:contains('Add a primary payment method')"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "events": [
                    "click"
                ],
                "selector": "a:contains('Add a primary payment method')",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "span[class='b3-additional-instrument-option-label']:contains('Add credit or debit card')"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "isRequired": true,
            "operation": {
                "events": [
                    "click"
                ],
                "selector": "span[class='b3-additional-instrument-option-label']:contains('Add credit or debit card')",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "input[name='cardnumber'][autocomplete='off-card-number'][data-was-visible='true']"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "isRequired": true,
            "operation": {
                "cardProperty": "cardNumber",
                "events": [
                    "focus",
                    "compositionstart",
                    "fillInputData",
                    "input",
                    "change",
                    "compositionend",
                    "blur",
                    "focusout"
                ],
                "selector": "input[name='cardnumber'][autocomplete='off-card-number'][data-was-visible='true']",
                "type": "fire_events"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "input[name='expirationDate-month'][type='tel'][autocomplete='cc-exp-month']"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "isRequired": true,
            "operation": {
                "type": "autofill_card_data"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "div[role=\"button\"][data-was-visible=\"true\"]:contains(\"submit\")"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "isRequired": true,
            "operation": {
                "events": [
                    "focus",
                    "DOMFocusIn",
                    "blur",
                    "DOMFocusOut",
                    "click",
                    "DOMActivate"
                ],
                "selector": "div[role=\"button\"][data-was-visible=\"true\"]:contains(\"submit\")",
                "type": "fire_events"
            },
            "retryConditional": []
        },
        {
            "conditional": [
                {
                    "watchedElementSelectors": [
                        {
                            "sizzleSelector": "div:textEquals('Successfully updated your payment method.')"
                        }
                    ]
                }
            ],
            "executionType": "separate"
        }
    ],
    "testing": true,
    "type": "BUSINESS"
}