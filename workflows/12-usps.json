{
    "category": "productivity",
    "enabled": false,
    "historyDetectionUrl": "",
    "hostname": "usps.com",
    "id": "",
    "name": "USPS",
    "runInFocusMode": false,
    "steps": [
        {
            "executionType": "separate",
            "help": {
                "message": "We're having trouble finding the billing page.",
                "title": "Trouble Finding Billing Page"
            },
            "info": {
                "message": "We are filling your billing details",
                "title": "Filling Billing Details"
            },
            "isRequired": true,
            "operation": {
                "ignoreFailure": true,
                "type": "navigate",
                "url": "https://pay.usps.com/pay/manageAccounts.html"
            }
        },
        {
            "conditional": [
                {
                    "urlSelectors": [
                        "https://reg.usps.com/informeddelivery/login"
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "message": "Please sign in to your USPS account.",
                "successConditional": [
                    {
                        "urlSelectors": [
                            "https://pay.usps.com/pay/manageAccounts.html"
                        ]
                    }
                ],
                "type": "await_user_help"
            }
        },
        {
            "executionType": "linked",
            "operation": {
                "ignoreFailure": true,
                "type": "navigate",
                "url": "https://pay.usps.com/pay/manageAccounts.html"
            }
        },
        {
            "conditional": [
                {
                    "elementSelectors": [
                        {
                            "sizzleSelector": "a[id=\"stored-card-add-btn\"]"
                        }
                    ]
                }
            ],
            "executionType": "separate",
            "operation": {
                "events": ["click"],
                "selector": "a[id=\"stored-card-add-btn\"]",
                "type": "fire_events"
            }
        }
    ],
    "testing": true,
    "type": "BUSINESS"
}